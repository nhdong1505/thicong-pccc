{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#1e40af",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#0ea5e9",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#0f172a",
					"name": "Dark",
					"slug": "dark"
				},
				{
					"color": "#f8fafc",
					"name": "Light",
					"slug": "light"
				}
			]
		},
		"layout": {
			"contentSize": "720px",
			"wideSize": "1140px"
		},
		"spacing": {
			"blockGap": "1.25rem",
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.925rem",
					"slug": "sm"
				},
				{
					"name": "Base",
					"size": "1rem",
					"slug": "base"
				},
				{
					"name": "Large",
					"size": "1.25rem",
					"slug": "lg"
				},
				{
					"name": "XL",
					"size": "1.875rem",
					"slug": "xl"
				},
				{
					"name": "2XL",
					"size": "2.5rem",
					"slug": "2xl"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "#ffffff",
			"text": "#0f172a"
		},
		"typography": {
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 3
}